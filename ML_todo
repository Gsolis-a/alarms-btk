\copy (select tipo_tx, tipo_tarjeta, cod_comercio, pan, n_cuotas, monto, msg_visor, estado_tx, hora_ini_switch, cliente_id, tipo_tarjeta from transacciones_credito where hora_ini_switch between '2022-04-26' and '2022-05-03' and select * from transacciones_credito tc LEFT OUTER JOIN binbase bb ON (substr(tc.pan, 1, 6)=bb.bin)LIMIT 3 and (tipo_tarjeta = '-1' or tipo_tarjeta in (select id_tarjeta from adquirencia_tarjetas))) to 'data.csv' delimiter ';' csv header;

